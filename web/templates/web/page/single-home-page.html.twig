{% extends 'base.html.twig' %}

{% block body %}
    {% if page.data.hide_title.0 is not defined or page.data.hide_title.0 != 'yes' %}
        <h1>{{ page.title }}</h1>
    {% endif %}

    {% if(page.content) %}
        {{ easy_gutenberg(page.content) }}
    {% endif %}

    {{ easy_gutenberg_assets() }}
{% endblock %}